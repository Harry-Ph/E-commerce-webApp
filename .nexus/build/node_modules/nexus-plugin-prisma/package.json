{
  "name": "nexus-plugin-prisma",
  "version": "0.10.0",
  "license": "MIT",
  "author": "Jason Kuhrt",
  "main": "dist/index.js",
  "module": "dist/mylib.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "scripts",
    "global-type.d.ts",
    "client.d.ts",
    "client.js",
    "typegen.d.ts"
  ],
  "scripts": {
    "format": "prettier --write .",
    "dev": "node scripts/build-module-facades && tsc --watch",
    "dev:test": "jest --watch src",
    "build": "yarn clean && node scripts/update-hash.js && node scripts/build-module-facades && tsc",
    "build:ci": "tsc",
    "test": "jest --testTimeout=400000 --forceExit",
    "test:unit": "jest src --forceExit",
    "test:e2e:mysql": "yarn test test/e2e/mysql.test.ts",
    "test:e2e:postgres": "yarn test test/e2e/postgres.test.ts",
    "test:e2e:sqlite": "yarn test test/e2e/sqlite.test.ts",
    "test:setup:db": "docker-compose down && rm -rf .mysql-data && rm -rf .postgres-data && docker-compose up -d",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "release:stable": "dripip stable",
    "release:preview": "dripip preview",
    "release:pr": "dripip pr",
    "prepack": "yarn -s build",
    "postinstall": "node ./scripts/postinstall.js"
  },
  "dependencies": {
    "@nexus/schema": "^0.14.0",
    "@prisma/cli": "^2.0.1",
    "@prisma/client": "^2.0.1",
    "@prisma/sdk": "^2.0.1",
    "chalk": "^4.0.0",
    "common-tags": "^1.8.0",
    "dotenv": "^8.2.0",
    "fs-jetpack": "^2.4.0",
    "graphql": "^14.6.0",
    "nexus-prisma": "^0.15.0",
    "prismjs": "^1.20.0",
    "stacktrace-parser": "^0.1.10"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@prisma-labs/prettier-config": "0.1.0",
    "@types/common-tags": "1.8.0",
    "@types/jest": "26.0.0",
    "@types/prismjs": "1.16.1",
    "dripip": "0.9.0",
    "jest": "26.0.1",
    "jest-watch-typeahead": "0.6.0",
    "nexus": "0.24.0",
    "node-fetch": "2.6.0",
    "node-pty": "0.9.0",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1",
    "rxjs": "6.5.5",
    "strip-ansi": "6.0.0",
    "ts-jest": "26.1.0",
    "tslib": "2.0.0",
    "typescript": "3.9.5"
  },
  "prisma": {
    "version": "7d77a7cd570c9dedb4cdf31a1cbec615cf2e1eda"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "prettier": "@prisma-labs/prettier-config"
}
