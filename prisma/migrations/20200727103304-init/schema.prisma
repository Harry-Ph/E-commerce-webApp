generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = "***"
}

model Ppl {
  email    String  @unique
  id       String  @id @default(cuid())
  password String
  role     String
  status   Boolean @default(true)
  username String
}

model Product {
  id   String @id @default(cuid())
  name String
}
