datasource db {
  provider = "postgresql"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model Ppl {
  id        String @default(cuid()) @id
  username  String
  email     String   @unique
  password  String
  role      String
  status    Boolean
  createdAt DateTime
  updatedAt DateTime
}

model Product {
  id   String @default(cuid()) @id
  name String
}